#>>>>>>>>>>>>>>>>>>>>>>>搜索

# bd  xxx (百度搜索)
^https:\/\/duckduckgo.com\/\?q=bd\+([^&]+).+ 302 https://www.baidu.com/s?wd=$1
^https:\/\/duckduckgo.com\/\?q=((.(?!bd))+)\+bd.+ 302 https://www.baidu.com/s?wd=$1

# wk  xxx (维基搜索)
^https:\/\/duckduckgo.com\/\?q=wk\+([^&]+).+ 302 https://zh.wikipedia.org/wiki/$1
^https:\/\/duckduckgo.com\/\?q=([^+]+)\+wk.+ 302 https://zh.wikipedia.org/wiki/$1

# by  xxx (必应)
^https:\/\/duckduckgo.com\/\?q=by\+([^&]+).+ 302 https://cn.bing.com/search?q=$1
^https:\/\/duckduckgo.com\/\?q=([^+]+)\+by.+ 302 https://cn.bing.com/search?q=$1

# ios  xxx (苹果应用搜索)
^https:\/\/duckduckgo.com\/\?q=ios\+([^&]+).+ 302 https://www.qimai.cn/search/index/search/$1
^https:\/\/duckduckgo.com\/\?q=([^+]+)\+ios.+ 302 https://www.qimai.cn/search/index/search/$1

#     xxx （无指令默认为 Google）
^https:\/\/duckduckgo.com\/\?q=([^&]+).+ 302 https://www.google.com/search?q=$1

[Mitm]
hostname = duckduckgo.com
